{
    "name": "n8n-mcp-server",
    "version": "1.0.0",
    "description": "Model Context Protocol server for n8n automation platform integration",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "node index.js",
        "health": "curl -s http://localhost:3001/health | jq",
        "dev": "DEBUG=1 node index.js",
        "docker:build": "docker build -t n8n-mcp-server .",
        "docker:run": "docker run -d --name n8n-mcp-server -p 3001:3001 --env-file .env n8n-mcp-server",
        "docker:stop": "docker stop n8n-mcp-server && docker rm n8n-mcp-server",
        "docker:logs": "docker logs -f n8n-mcp-server",
        "compose:up": "docker-compose up -d",
        "compose:down": "docker-compose down",
        "compose:logs": "docker-compose logs -f",
        "compose:restart": "docker-compose restart"
    },
    "keywords": [
        "n8n",
        "mcp",
        "model-context-protocol",
        "ai",
        "automation",
        "workflow",
        "claude",
        "anthropic"
    ],
    "author": "sonnd08",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.0.0",
        "axios": "^1.7.7",
        "dotenv": "^16.4.7"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sonnd08/n8n-mcp"
    }
}